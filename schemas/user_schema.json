{
    "$schema": "https://json-schema.org/draft-04/schema",
    "id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "required": [
        "data",
        "support"
    ],
    "properties": {
        "data": {
            "title": "The data Schema",
            "type": "object",
            "required": [
                "id",
                "email",
                "first_name",
                "last_name",
                "avatar"
            ],
            "properties": {
                "id": {
                    "title": "The id Schema",
                    "type": "integer"
                },
                "email": {
                    "title": "The email Schema",
                    "type": "string"
                },
                "first_name": {
                    "title": "The first_name Schema",
                    "type": "string"
                },
                "last_name": {
                    "title": "The last_name Schema",
                    "type": "string"
                },
                "avatar": {
                    "title": "The avatar Schema",
                    "type": "string"
                }
            }
        },
        "support": {
            "title": "The support Schema",
            "type": "object",
            "required": [
                "url",
                "text"
            ],
            "properties": {
                "url": {
                    "title": "The url Schema",
                    "type": "string"
                },
                "text": {
                    "title": "The text Schema",
                    "type": "string"
                }
            }
        }
    }
}